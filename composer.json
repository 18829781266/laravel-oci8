{
    "name": "crazycodr/laravel-pdo-via-oci8",
    "description": "Oracle DB driver for Laravel 4 using CrazyCodr's PDO userspace OCI8 driver",
    "keywords": ["oracle", "laravel"],
    "license": "MIT",
    "authors": [
        {
            "name": "CrazyCodr - Mathieu Dumoulin",
            "email": "crazyone@crazycoders.net"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "illuminate/support": "4.0.x",
        "illuminate/database": "4.0.x"
    },
    "require-dev": {
        "mockery/mockery": "0.7.2",
        "phpunit/phpunit": "3.7.*"
    },
    "autoload": {
        "psr-0": {
            "CrazyCodr\\Laravel\\PdoViaOci8": "src/"
        }
    },
    "minimum-stability": "dev"
}